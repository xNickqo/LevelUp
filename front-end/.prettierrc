{
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "none",
  "endOfLine": "auto",
  "printWidth": 130,
  "semi": true,
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "singleQuote": false,
        "parser": "angular",
        "tabWidth": 4
      }
    },
    {
      "files": "*.json",
      "options": {
        "singleQuote": false,
        "tabWidth": 4
      }
    }
  ]
}
