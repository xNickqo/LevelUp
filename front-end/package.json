{
  "name": "jenga",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start_docker": "ng serve --disable-host-check --host 0.0.0.0 --port 4200 --poll 200 ",
    "build": "ng build",
    "build_dev": "npm run build -- --configuration=dev",
    "build_pre": "npm run build -- --configuration=pre",
    "build_production": "npm run build -- --configuration=production",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "test": "ng test",
    "test:ci": "ng test --watch=false --code-coverage",
    "extract:translations": "ngx-translate-extract --input ./src --output ./src/assets/i18n/es.json ./src/assets/i18n/en.json --sort --format namespaced-json",
    "release:first": "standard-version --no-verify --first-release",
    "release:patch": "standard-version --no-verify --release-as patch",
    "release:minor": "standard-version --no-verify --release-as minor",
    "release:major": "standard-version --no-verify --release-as major",
    "bundle:report": "source-map-explorer dist/jenga/browser/**/*.js"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run test:ci",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~19.2.0",
    "@angular/cdk": "~19.2.3",
    "@angular/common": "~19.2.0",
    "@angular/compiler": "~19.2.0",
    "@angular/core": "~19.2.0",
    "@angular/fire": "^19.1.0",
    "@angular/forms": "~19.2.0",
    "@angular/material": "~19.2.1",
    "@angular/platform-browser": "~19.2.0",
    "@angular/platform-browser-dynamic": "~19.2.0",
    "@angular/router": "~19.2.0",
    "@colsen1991/ngx-translate-extract-marker": "^3.0.1",
    "@ngx-translate/core": "~15.0.0",
    "@ngx-translate/http-loader": "~8.0.0",
    "@primeng/themes": "^19.1.0",
    "@stripe/stripe-js": "^7.3.0",
    "@tailwindcss/postcss": "^4.1.7",
    "firebase": "^11.7.1",
    "jwt-decode": "^4.0.0",
    "postcss": "^8.5.3",
    "primeicons": "^7.0.0",
    "primeng": "^19.1.0",
    "rxjs": "~7.8.0",
    "tslib": "~2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~19.2.0",
    "@angular-eslint/builder": "^18.0.0",
    "@angular-eslint/eslint-plugin": "^18.0.0",
    "@angular-eslint/eslint-plugin-template": "^18.0.0",
    "@angular-eslint/schematics": "^18.0.0",
    "@angular-eslint/template-parser": "^18.0.0",
    "@angular/cli": "~19.2.0",
    "@angular/compiler-cli": "~19.2.0",
    "@bartholomej/ngx-translate-extract": "~8.0.2",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "~20.14.9",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.3.6",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^4.3.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^2.8.8",
    "prettier-eslint": "^15.0.1",
    "source-map-explorer": "^2.5.0",
    "standard-version": "~9.5.0",
    "tailwindcss": "^4.1.7",
    "typescript": "~5.8.2"
  }
}
